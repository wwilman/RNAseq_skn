#!/bin/bash   

file="/A_SKN//"  

out="/A_SKN/dane_trimmed/"  

fr="ERR4131"  

type=".fastq.gz"  

ml=60  

slid='4:20'  

for n in 950 951 952 953 966 967 968 969

do   

echo "===================================================="  

echo 'Trimming File:' $fr$n$type    

echo "====================================================" java -jar trimmomatic-0.36.jar SE -phred33 $file$fr$n$type $out$fr$n$type SLIDINGWINDOW:$slid MINLEN:$ml  

done 
